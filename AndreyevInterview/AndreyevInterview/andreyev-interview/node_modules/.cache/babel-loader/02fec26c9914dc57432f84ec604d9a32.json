AO,CAAC;AAA3C;AAAA;AAEF,KAFd,E;0BAAkE;AAAA,eAElE,C,UAAA,CAFkE;AAAA,O;;;;KAAlE,E;;AAAA,M,MAAA,CAGG,CAJL,C,CANF,C;GAWK,CAXL,E;;AAAA,GAYM,EAbR,CAcM,CAvBR,CAwBI,CAjCN,C","sourcesContent":["<template>\n  <div class=\"home\">\n    <form @submit.prevent>\n      <label for=\"invoices\">Create a new invoice</label>\n      <input type=\"text\" name=\"invoices\" v-model=\"state.description\" placeholder=\"Description\" />\n      <el-button @click=\"createInvoice\">Create Invoice</el-button>\n    </form>\n\n    <hr />\n\n    <table>\n      <thead>\n        <th>ID</th>\n        <th>Description</th>\n        <th>Total Value</th>\n        <th>Total Billable Value</th>\n        <th>Total Number Line Items</th>\n        <th></th>\n      </thead>\n      <tbody>\n        <tr v-for=\"invoice in state.invoices\" :key=\"invoice.id\">\n          <td>{{invoice.id}}</td>\n          <td>{{invoice.description}}</td>\n          <td> {{invoice.totalValue}}</td>\n          <td> {{invoice.totalBillableValue}}</td>\n          <td> {{invoice.totalNumberLineItems}}</td>\n          <td>\n            <router-link :to=\"{ name: 'Invoice', params: { id: invoice.id }}\">\n              Open\n            </router-link>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { defineComponent, onMounted, reactive } from 'vue';\n// @ is an alias to /src\n\nexport default defineComponent({\n  name: 'Invoices',\n  setup() {\n    const state = reactive({\n      invoices: [],\n      description: \"\"\n    })\n\n    function fetchInvoices() {\n      fetch(\"http://localhost:5000/invoices\", {\n        method: \"GET\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n      }).then((response) => {\n        response.json().then(invoices => (state.invoices = invoices))\n      })\n    }\n\n    function createInvoice() {\n      fetch(\"http://localhost:5000/invoices\", {\n        method: \"PUT\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n        body: JSON.stringify({\n          description: state.description\n        })\n      }).then(fetchInvoices)\n    }\n\n    onMounted(fetchInvoices)\n\n    return {state, createInvoice}\n  }\n});\n</script>\n"],"sourceRoot":""}]}