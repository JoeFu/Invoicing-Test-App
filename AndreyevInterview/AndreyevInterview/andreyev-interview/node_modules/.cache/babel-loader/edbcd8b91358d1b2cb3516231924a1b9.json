AsC;;aAAlB,IAAA,CAAA,KAAA,CAAM,QAAN,GAAc,M;;AAAI,GAAvF,E,IAAA,E;;AAAA,G,iBAAqE,IAAA,CAAA,KAAA,CAAM,Q,iCAC/E,YAAA,CAA2E,OAA3E,EAA2E;AAApE,IAAA,IAAI,EAAC,QAA+D;AAAtD,IAAA,IAAI,EAAC,MAAiD;AAA1C,IAAA,WAAW,EAAC,MAA8B;;aAAd,IAAA,CAAA,KAAA,CAAM,IAAN,GAAU,M;;AAAI,GAA3E,E,IAAA,E;;AAAA,G,iBAA6D,IAAA,CAAA,KAAA,CAAM,I,KACzE,YAAA,CAAuD,QAAvD,EAAuD;AAA9C,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,IAAA,CAAA,cAAA,IAAA,IAAA,CAAA,cAAA,OAAA,IAAA,YAAF;AAAA,KAAA;AAAyC,GAAvD,EAAgC,gBAAhC,C,CALF,E;;AAAA,GAOI,CAjCN,C","sourcesContent":["<template>\n  <div class=\"about\">\n    <router-link :to=\"{ name: 'Invoices' }\">Back</router-link>\n\n    <h2>Invoice Details</h2>\n\n    <span>Invoice #{{$route.params.id}}</span>\n\n    <h3>Line Items</h3>\n\n    <table>\n      <thead>\n        <th>ID</th>\n        <th>Description</th>\n        <th>Quantity</th>\n        <th>Cost</th>\n      </thead>\n      <tbody>\n        <tr v-for=\"item in state.lineItems\" :key=\"item.id\">\n          <td>{{item.id}}</td>\n          <td>{{item.description}}</td>\n          <td>{{item.quantity}}</td>\n          <td>{{item.cost}}</td>\n        </tr>\n      </tbody>\n    </table>\n\n    <form @submit.prevent>\n      <h4>Create Line Item</h4>\n      Description:  <input type=\"text\" name=\"description\" placeholder=\"Description\" v-model=\"state.description\" />\n      Quantity: <input type=\"number\" name=\"quantity\" placeholder=\"Quantity\" v-model=\"state.quantity\" />\n      Cost: <input type=\"number\" name=\"cost\" placeholder=\"Cost\" v-model=\"state.cost\" />\n      <button @click=\"createLineItem\">Create Invoice</button>\n    </form>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport {defineComponent, onMounted, reactive} from \"vue\";\n\nexport default defineComponent({\n  name: \"Invoice\",\n  props: {\n    id: {\n      type: [String, Number],\n      required: true\n    }\n  },\n  setup(props) {\n    const state = reactive({\n      lineItems: [],\n      description: \"\",\n      quantity: \"0\",\n      cost: \"0\"\n    })\n\n    function fetchLineItems() {\n      fetch(`http://localhost:5000/invoices/${props.id}`, {\n        method: \"GET\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n      }).then((response) => {\n        response.json().then(lineItems => (state.lineItems = lineItems))\n      })\n    }\n\n    function createLineItem() {\n      fetch(`http://localhost:5000/invoices/${props.id}`, {\n        method: \"POST\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n        body: JSON.stringify({\n          description: state.description,\n          quantity: Number(state.quantity),\n          cost: Number(state.cost)\n        })\n      }).then(fetchLineItems)\n    }\n\n    onMounted(fetchLineItems)\n\n    return {state, createLineItem}\n  }\n})\n</script>"],"sourceRoot":""}]}